{
  "name": "bne-support",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "wrangler dev api/src/index.ts --port 8787",
    "dev:both": "concurrently \"npm run dev\" \"npm run dev:api\"",
    "build": "tsc -b && vite build",
    "deploy:api": "npx drizzle-kit migrate && wrangler deploy api/src/index.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "geocode": "tsx scripts/geocode-locations.ts"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20250719.0",
    "@hono/zod-validator": "^0.7.2",
    "@radix-ui/react-aspect-ratio": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@react-google-maps/api": "^2.20.6",
    "@tailwindcss/vite": "^4.0.6",
    "better-sqlite3": "^12.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.3",
    "hono": "^4.8.5",
    "lucide-react": "^0.475.0",
    "luxon": "^3.5.0",
    "next-themes": "^0.4.6",
    "papaparse": "^5.5.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-resizable-panels": "^2.1.7",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^4.0.6",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.2",
    "wrangler": "^4.25.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.13.4",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "concurrently": "^9.2.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "prettier": "^3.6.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0"
  }
}
